    "\n",
    "dic2 = dict(zip(lst1, lst2))\n",
    "cnt = 0\n",
    "while dic2[a] != dic2[b]:\n",
    "    for key in dic2:\n",
    "        dic2[key] = dic2[key]//2\n",
    "    cnt += 1\n",
    "print(cnt)"
