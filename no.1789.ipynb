    "S = int(input())\n",
    "\n",
    "\n",
    "def total(n):\n",
    "    return int(n * (n + 1) / 2)\n",
    "\n",
    "\n",
    "n = 0\n",
    "while total(n) <= S:\n",
    "    n += 1\n",
    "print(n - 1)"
